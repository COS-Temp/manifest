<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="cosmic-os"
          fetch="https://github.com/Cosmic-OS"
          revision="refs/heads/corona-release" />

  <remote name="devices"
          fetch="https://github.com/Cosmic-Devices"
          revision="refs/heads/corona-release" />

  <remote name="cosmic-os-temp"
